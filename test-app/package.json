{
  "name": "omise-mcp-test-app",
  "version": "1.0.0",
  "description": "Comprehensive test application for Omise MCP server with A2A authentication",
  "main": "dist/mcp-test-app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/mcp-test-app.js",
    "dev": "ts-node mcp-test-app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "mcp",
    "omise",
    "payment",
    "a2a",
    "authentication",
    "testing"
  ],
  "author": "Omise MCP Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "@faker-js/faker": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/../tests", "<rootDir>"],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
